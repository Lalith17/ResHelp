  "scripts": {
    "backend": "nodemon backend/server.js",
    "frontend": "cd frontend && npm run dev",
    "nlp": "cd backend/nlp && call nlp_env\\Scripts\\activate && uvicorn nlp_server:app --reload --port 8001",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run nlp\"",
    "docker:build": "docker-compose build",
